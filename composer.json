  "keywords":["ZF2", "wpscan", "eoko"],
  "type":"library",
  "license":"MIT",

  "minimum-stability": "dev",
  "prefer-stable": true,

  "authors":[
    {
      "name":"Romain DARY",
      "email": "romain.dary@eoko.fr",
      "homepage":"http://eoko.fr"
    }
  ],
  "homepage": "https://github.com/eoko/wpscan",

  "autoload": {
